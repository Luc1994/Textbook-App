	public static String compactSearch(String userinput) {
		
		if (userinput == null) {
			throw new IllegalArgumentException("You gotta put stuff in the search bar, dummy.");
		}
		else {
		String realinput;
		realinput = userinput;
		 
		
		return realinput;
		}
	}
 

	public static final Set<String> CommonWords;
	static  { CommonWords = new LinkedHashSet<String>();
	CommonWords.add("a");
	CommonWords.add("an");
	CommonWords.add("the");
	CommonWords.add("and");
	CommonWords.add("but");
	CommonWords.add("or");
	CommonWords.add("to");
	CommonWords.add("on");
	CommonWords.add("from");
	CommonWords.add("at");
	CommonWords.add("of");
	CommonWords.add("is");
	CommonWords.add("for");
	
	}
